{
  "hooks": {
    "SessionEnd": [
      {
        "hooks": [
          {
            "command": "tw hook session-end",
            "timeout": 5,
            "type": "command"
          }
        ]
      }
    ],
    "SessionStart": [
      {
        "hooks": [
          {
            "command": "tw hook session-init",
            "timeout": 10,
            "type": "command"
          }
        ]
      }
    ],
    "Stop": [
      {
        "hooks": [
          {
            "command": "tw hook continue-check --max-tasks=5 --max-minutes=30",
            "timeout": 15,
            "type": "command"
          }
        ]
      }
    ]
  }
}