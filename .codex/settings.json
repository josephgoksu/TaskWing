{
  "hooks": {
    "SessionEnd": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "taskwing hook session-end",
            "timeout": 5
          }
        ]
      }
    ],
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "taskwing hook session-init",
            "timeout": 10
          }
        ]
      }
    ],
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "taskwing hook continue-check --max-tasks=5 --max-minutes=30",
            "timeout": 15
          }
        ]
      }
    ]
  }
}